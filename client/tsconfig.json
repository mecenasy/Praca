{
   "compilerOptions": {
      "target": "ES2020",
      "lib": [
         "dom",
         "dom.iterable",
         "esnext"
      ],
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react",
   },
   "include": [
      "./",
   ],
}
